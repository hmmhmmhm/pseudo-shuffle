{
  "name": "psuedo-shuffle",
  "version": "0.0.1",
  "description": "Make the index look like it is shuffled according to the range so that it is not conflicted without the actual shuffle.",
  "type": "module",
  "source": "src/psuedo-shuffle.ts",
  "exports": {
    "require": "./dist/psuedo-shuffle.cjs",
    "default": "./dist/psuedo-shuffle.modern.js"
  },
  "main": "./dist/psuedo-shuffle.cjs",
  "module": "./dist/psuedo-shuffle.module.js",
  "unpkg": "./dist/psuedo-shuffle.umd.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle && node ./dist/psuedo-shuffle.cjs",
    "test": "microbundle && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmmhmmhm/psuedo-shuffle.git"
  },
  "keywords": [],
  "author": "hmmhmmhm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmmhmmhm/psuedo-shuffle/issues"
  },
  "homepage": "https://github.com/hmmhmmhm/psuedo-shuffle#readme",
  "dependencies": {
    "node-fe1-fpe": "^1.0.0-beta.2"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "jest": "^29.5.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.1.0"
  }
}
